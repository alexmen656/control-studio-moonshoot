# Worker ID (optional, will be auto-generated if not provided)
WORKER_ID=

# Worker Name (optional)
WORKER_NAME=Upload-Worker-1

# Backend URL
BACKEND_URL=https://localhost:3001

# Heartbeat interval in milliseconds (default: 30000 = 30 seconds)
HEARTBEAT_INTERVAL=30000

# Job polling interval in milliseconds (default: 5000 = 5 seconds)
JOB_POLL_INTERVAL=5000

# Maximum concurrent upload tasks
MAX_CONCURRENT_TASKS=3
